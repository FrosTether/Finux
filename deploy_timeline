import os
import subprocess

def deploy_timeline_module():
    print("‚è≥ DEPLOYING CHRONOS TIMELINE...")
    
    target = "GRAYSONS_WALLET_BUILD/chronos_timeline.py"
    
    if os.path.exists(target):
        cmds = [
            ["git", "add", target],
            ["git", "commit", "-m", "Feature: Biographical Timeline Key Generation"],
            ["git", "push", "origin", "main"]
        ]
        
        for cmd in cmds:
            try:
                subprocess.run(cmd, check=False)
                print(f"   [OK] {' '.join(cmd)}")
            except:
                pass
            
        print("\n‚úÖ UPLOAD COMPLETE.")
    else:
        print(f"   [!] Error: {target} not found.")

if __name__ == "__main__":
    deploy_timeline_module()
import os
import subprocess

def deploy_chronos():
    print("üß≥ PACKING FOR DEPLOYMENT (CHRONOS LAYER)...")
    
    # 1. Target File
    target = "GRAYSONS_WALLET_BUILD/chronos_key.py"
    
    if os.path.exists(target):
        cmds = [
            ["git", "add", target],
            ["git", "commit", "-m", "Feature: Chronos Temporal Key Generation (1992 Seed)"],
            ["git", "push", "origin", "main"]
        ]
        
        for cmd in cmds:
            subprocess.run(cmd, check=False)
            print(f"   [OK] {' '.join(cmd)}")
            
        print("\n‚úÖ TIME TRAVEL MODULE ONLINE.")
    else:
        print(f"   [!] Error: File {target} not found.")

if __name__ == "__main__":
    deploy_chronos()
